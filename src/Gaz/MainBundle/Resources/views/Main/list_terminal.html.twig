{% extends 'GazMainBundle::layout.html.twig' %}

 {% block contentMain %}
     <div class="container">
         <div class="row">
             {% for terminal in terminals %}
                 <a href="{{ path('terminal_create', {'id':terminal.id}) }}">
                 <button class="btn btn-primary" type="button">
                     Terminal number {{ terminal.number }} <br/>
                     Station number {{ terminal.station }}
                     <span class="badge">Amount Xy</span>
                 </button>
                 </a>
             {% endfor %}
         </div>
     </div>
 {% endblock %}